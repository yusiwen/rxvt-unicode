Window			T_UV

urxvt::pixbuf           T_PTROBJ
urxvt::img              T_PTROBJ

octet_string            OCTET_STRING
utf8_string             UTF8_STRING

rxvt_term *		TERM

GError **		T_UV

INPUT

TERM
	$var = SvTERM ($arg);
OVERLAY
	$var = SvOVERLAY ($arg);

OCTET_STRING
        $var = SvPVbyte_nolen ($arg)

UTF8_STRING
        $var = SvPVutf8_nolen ($arg) /* imperfect, destroys references */

OUTPUT

